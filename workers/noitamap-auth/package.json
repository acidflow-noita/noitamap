{
  "name": "noitamap-auth",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env dev",
    "db:create": "wrangler d1 create noitamap-auth",
    "db:create:dev": "wrangler d1 create noitamap-auth-dev",
    "db:migrate": "wrangler d1 migrations apply noitamap-auth",
    "db:migrate:dev": "wrangler d1 migrations apply noitamap-auth-dev --env dev",
    "db:migrate:local": "wrangler d1 migrations apply noitamap-auth-local --local"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260127.0",
    "typescript": "^5.4.5",
    "wrangler": "^3.57.0"
  }
}
