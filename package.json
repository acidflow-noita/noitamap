{
  "name": "noitamap",
  "version": "1.0.0",
  "description": "Noita Map",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup && npm run copy-flags && npm run add-biome-translations && npm run copy-locales",
    "copy-flags": "node build_scripts/copy-flags.cjs",
    "add-biome-translations": "node build_scripts/add-biome-translations.cjs",
    "copy-locales": "node build_scripts/copy-locales.cjs",
    "watch": "tsup --no-minify --watch src/**/*.ts --watch src/data/*.json",
    "generate": "node ./src/data/data_generators/map_definitions_maker.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acidflow-noita/noitamap.git"
  },
  "type": "module",
  "keywords": [
    "Map",
    "Noita",
    "Noita",
    "Map",
    "Noitamap",
    "OpenseaDragon"
  ],
  "author": "WUOTE",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/acidflow-noita/noitamap/issues"
  },
  "homepage": "https://github.com/acidflow-noita/noitamap#readme",
  "dependencies": {
    "@designsystemsinternational/flags": "^0.2.0",
    "@wtsml/doodle": "^1.0.4",
    "eventemitter2": "6.4.9",
    "flexsearch": "0.7.40",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "idb": "^8.0.3",
    "lz-string": "^1.5.0",
    "openseadragon": "5.0.1",
    "simplify-js": "^1.2.4"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.7",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^22.7.7",
    "@types/openseadragon": "^3.0.10",
    "openseadragon": "^5.0.1",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
